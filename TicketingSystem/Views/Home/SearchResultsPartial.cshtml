@model List<TicketingSystem.Models.TicketsRepository.TicketsList2>

<div class="row">
    <div class="col-md-4">
        <h2>Title</h2>
    </div>
    <div class="col-md-4">
        <h2>Category</h2>
    </div>
    <div class="col-md-4">
        <h2>Author</h2>
    </div>
    <div class="col-md-4">
        <h2>Priority</h2>
    </div>
</div>
    @foreach (var result in Model)
    {
        <div class="row">
            <div class="col-md-4">
                <h2>@result.Title</h2>
            </div>
            <div class="col-md-4">
                <h2>@result.Category</h2>
            </div>
            <div class="col-md-4">
                <h2>@result.Author</h2>
            </div>
            <div class="col-md-4">
                <h2>@result.Priority</h2>
            </div>
        </div>
    }
